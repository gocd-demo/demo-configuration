{
  "format_version": 1,
  "label_template": "${COUNT}",
  "enable_pipeline_locking": false,
  "name": "json_installers",
  "template": null,
  "group": "Development",

  "materials": [
    {
      "type": "git",
      "url": "https://github.com/gocd-demo/jy_main_repo.git",
      "name": "mainGit"
    },
    {
      "type": "dependency",
      "pipeline": "build_and_test",
      "stage": "test",
      "name": "build_and_test",
      "auto_update": true,
      "name": "build_pipeline"
    }
  ],
  "stages": [
    {
      "name": "defaultStage",
      "fetch_materials": true,
      "jobs": [
        {
          "name": "defaultJob",
          "elastic_profile_id": "docker-maven",
          "tasks": [
            {
              "type": "exec",
              "on_cancel": null,
              "command": "mvn",
              "arguments": [
                "clean",
                "package",
                "-DskipTests"
              ],
              "working_directory": null
            }
          ],
          "tabs": [

          ],
          "artifacts": [
            {
              "source": "target/*.jar",
              "destination": "distribution",
              "type": "build"
            }
          ],
          "properties": null
        }
      ]
    }
  ]
}
